    "babelify": "^6.1.3",
    "browserify": "^11.0.1"
  },
  "dependencies": {
    "core-js": "^1.0.1",
    "eventemitter3": "^1.1.1",
    "raf-stream": "^3.0.0",
    "vkey": "^1.0.0"
  }
