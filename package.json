{
  "name": "quay",
  "version": "0.2.0",
  "description": "Clientside module for turning keypresses into event streams",
  "main": "lib",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepack": "npm run build",
    "prebuild": "rm -rf lib && mkdir lib",
